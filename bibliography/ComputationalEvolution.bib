	Journal = {Infect Genet Evol},
	Journal-Full = {Infection, genetics and evolution : journal of molecular epidemiology and evolutionary genetics in infectious diseases},
	Mesh = {Algorithms; Computer Simulation; Data Interpretation, Statistical; Empirical Research; Phenotype; Phylogeny; Uncertainty; Virus Physiological Phenomena; Viruses},

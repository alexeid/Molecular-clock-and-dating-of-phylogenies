	Pmid = {24037268},
	Pst = {ppublish},
	Title = {Relating phylogenetic trees to transmission trees of infectious disease outbreaks},
	Volume = {195},
	Year = {2013},
	Bdsk-Url-1 = {http://dx.doi.org/10.1534/genetics.113.154856}}


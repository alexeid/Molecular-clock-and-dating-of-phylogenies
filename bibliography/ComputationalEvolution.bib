	Doi = {10.1073/pnas.0904991106},
	Journal = {Proc Natl Acad Sci U S A},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Mesh = {Bayes Theorem; Cluster Analysis; Disease Outbreaks; Evolution, Molecular; History, 20th Century; Humans; Influenza A virus; Influenza, Human; Models, Genetic; Phylogeny},

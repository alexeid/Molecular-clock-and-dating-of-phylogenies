	Journal = {Mol Biol Evol},
	Journal-Full = {Molecular biology and evolution},
	Mesh = {Animals; Codon; Evolution, Molecular; Genetic Variation; Humans; Influenza A virus; Likelihood Functions; Models, Genetic; Phylogeny; Selection, Genetic},
	Month = {Aug},

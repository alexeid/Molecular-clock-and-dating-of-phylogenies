	Number = {3},
	Pages = {e1002947},
	Pmc = {PMC3605911},
	Pmid = {23555203},
	Pst = {ppublish},
	Title = {Viral phylodynamics},
	Volume = {9},
	Year = {2013},
	Bdsk-Url-1 = {http://dx.doi.org/10.1371/journal.pcbi.1002947}}


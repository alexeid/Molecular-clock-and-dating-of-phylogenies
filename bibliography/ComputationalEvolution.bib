	Number = {19},
	Pages = {12515--12527},
	Title = {Simian Immunodeficiency Virus Infection in {Free-Ranging} Sooty Mangabeys {(Cercocebus} atys atys) from the Tai Forest, Cote {d'Ivoire:} Implications for the Origin of Epidemic Human Immunodeficiency Virus Type 2},
	Url = {http://jvi.asm.org/cgi/content/abstract/79/19/12515},

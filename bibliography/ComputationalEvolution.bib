	Keywords = {phylogenetic tree, quartet, Reassortment, segmented genome, virus},
	Number = {1-2},
	Pages = {11--16},
	Title = {A phylogenetic approach to detecting reassortments in viruses with segmented genomes},
	Url = {http://www.sciencedirect.com/science/article/B6T39-505Y0NP-1/2/0c14c4f98adfad5f4ed95aa2014ec0e9},

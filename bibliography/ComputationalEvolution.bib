	Mesh = {Bayes Theorem; Communicable Diseases; Evolution, Molecular; Genome-Wide Association Study; Humans; Likelihood Functions; Markov Chains; Methicillin-Resistant Staphylococcus aureus; Models, Genetic; Phylogeny; Phylogeography; Staphylococcal Infections},
